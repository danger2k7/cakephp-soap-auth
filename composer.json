{
    "name": "danger2k7/cakephp-soap-auth",
    "type": "cakephp-plugin",
    "description": "CakePHP plugin for authenticating  for stateless connexions",
    "keywords": [
        "cakephp",
        "authenticate",
        "authentication",
        "soap",
        "stareless"
    ],
    "homepage": "https://github.com/danger2k7/cakephp-soap-auth",
    "authors": [
        {
            "name":"Bogdan SOOS",
            "role":"Author",
            "homepage":"https://github.com/dynweb-org"
        }
    ],
    "license": "MIT",
    "support": {
        "source":"https://github.com/danger2k7/cakephp-soap-auth",
        "issues":"https://github.com/danger2k7/cakephp-soap-auth/issues"
    },
    "require": {
        "zendframework/zend-diactoros": "~1.0",
        "cakephp/cakephp": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Dynweb\\SoapAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dynweb\\SoapAuth\\Test\\": "tests"
        }
    }
}
